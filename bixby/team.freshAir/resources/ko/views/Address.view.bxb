result-view {
  match {
    addressConcept (address)
  }

  render {
    layout {
      section {
        content {
          single-line {
            text {
              style (Detail_L)
              value ("One: #{value(address.levelOne)}")
            }
          }
          
          single-line {
            text {
              style (Detail_L)
              value ("Two: #{value(address.levelTwo)}")
            }
          }
        }
      }
    }
  }
}