result-view {
  match {
    TodayPM100Concept (todayPM100)
  }

  render {
    layout {
      section {
        content {
          for-each (todayPM100.PM100) {
            as (PM100) {
              single-line {
                text {
                  style (Detail_M)
                  value ("미세먼지: #{value(PM100)}")
                }
              }
            }
          }
        }
      }
    }
  }
}