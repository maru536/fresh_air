result-view {
  match {
    appUrlConcept (appUrl)
  }
  
  app-launch {
    payload-uri ("#{value(appUrl)}")
    requires {
      app-id (com.perfect.freshair)
      app-name (미세코치)
      app-min-version (1)
      app-store-url (https://play.google.com/store/apps/details?id=com.perfect.freshair)
    }
  }
}