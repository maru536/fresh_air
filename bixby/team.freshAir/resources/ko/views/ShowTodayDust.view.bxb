result-view {
  match: dustSummaryConcept (dustSummary) {
    from-output: ShowTodayDust
  }
  
  render {
    layout {
      if (exists(dustSummary)) {
        layout-macro (dustSummaryLayout) {
          param (dustSummary) {
            expression (dustSummary)
          }
        }
      }
    }
  }
  
  conversation-drivers {
    conversation-driver {
      template ("어제 미세먼지 보여줘")
    }
  }
}