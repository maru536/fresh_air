result-view {
  match {
    latestDustConcept (latestDust)
  }

  render {
    layout {
      layout-macro (latestDustResult) {
        param (latestDust) {
          expression (latestDust)
        }
      }
    }
  }
  
  conversation-drivers {
    conversation-driver {
      template ("코치 해줘")
    }
  }
}