action (ShowLatestDust) {
  description (Print latest dust measurement to user)
  collect {
    input (userId) {
      type (userIdConcept)
      min (Required) max (One)
      default-init {
        intent {
          goal: SaveUserId
        }
      }
    }
  }
  
  output (latestDustConcept)
  type (Calculation)
}
